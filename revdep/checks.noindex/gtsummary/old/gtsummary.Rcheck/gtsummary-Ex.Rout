+   t.test(data[[variable]] ~ as.factor(data[[by]])) |>
+     broom::tidy() %>%
+     dplyr::mutate(
+       stat = glue::glue("t={style_sigfig(statistic)}, {style_pvalue(p.value, prepend_p = TRUE)}")
+     ) %>%
+     dplyr::pull(stat)
+ }

> # Example 1 ----------------------------------
> # show 'grade' p-values to 3 decimal places and estimates to 4 sig figs
> lm(age ~ marker + grade, trial) |>
+   tbl_regression() %>%
+   modify_fmt_fun(
+     p.value = label_style_pvalue(digits = 3),
+     c(estimate, conf.low, conf.high) ~ label_style_sigfig(digits = 4),
+     rows = variable == "grade"
+   )
